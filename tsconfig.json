    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "react",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "outDir": "./build"
    }
}
